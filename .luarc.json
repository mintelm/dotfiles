{
  "workspace.checkThirdParty": false,
  "format.defaultConfig": {
    "indent_style": "space",
    "indent_size": "4",
    "max_line_length": "140",
    "quote_style": "single"
  },
  "diagnostics.neededFileStatus": {
    "codestyle-check": "Any"
  },
  "diagnostics.globals": [ "vim" ],
    "diagnostics.disable": [
        "missing-fields"
    ]
}
